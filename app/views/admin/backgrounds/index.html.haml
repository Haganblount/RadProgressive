= s3_uploader_form callback_url: admin_backgrounds_path, callback_param: 'background[image_url]', id: 's3-uploader', max_file_size: 3.megabytes do
  
  = file_field_tag :file, multiple: true

  .files.sort-photos{ data: { url: positions_admin_backgrounds_path } }
    = render @backgrounds
  

= render 'admin/backgrounds/upload_tmpl'
